<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>tipvehiControlador</servlet-name>
        <servlet-class>Controlador.tipvehiControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vehiControlador</servlet-name>
        <servlet-class>Controlador.vehiControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tipconducControlador</servlet-name>
        <servlet-class>Controlador.tipconducControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rutaControlador</servlet-name>
        <servlet-class>Controlador.rutaControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contratoControlador</servlet-name>
        <servlet-class>Controlador.contratoControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conductorControlador</servlet-name>
        <servlet-class>Controlador.conductorControlador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tipvehiControlador</servlet-name>
        <url-pattern>/tipovehi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vehiControlador</servlet-name>
        <url-pattern>/vehiculo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contratoControlador</servlet-name>
        <url-pattern>/contratoControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conductorControlador</servlet-name>
        <url-pattern>/conductorControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tipconducControlador</servlet-name>
        <url-pattern>/tipoConductor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rutaControlador</servlet-name>
        <url-pattern>/ruta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
